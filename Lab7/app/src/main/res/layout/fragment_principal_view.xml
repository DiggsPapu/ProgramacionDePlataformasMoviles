<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".PrincipalView">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/TextV_FMain_Title"
        android:textSize="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="@string/Lab"
        android:textStyle="bold">
    </TextView>
    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/TextInput_FMain_Input"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/TextV_FMain_Title"
        app:layout_constraintStart_toStartOf="@+id/TextV_FMain_Title"
        app:layout_constraintEnd_toEndOf="@+id/TextV_FMain_Title"
        >
        <com.google.android.material.textfield.TextInputEditText
            android:id="TextInputEdit_FMain_Input"
            android:layout_height="30dp"
            android:layout_width="200dp"


            >

        </com.google.android.material.textfield.TextInputEditText>

    </com.google.android.material.textfield.TextInputLayout>



</androidx.constraintlayout.widget.ConstraintLayout>